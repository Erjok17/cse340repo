<section class="delorean-section">
  <img src="/images/vehicles/delorean.jpg" alt="DMC Delorean">
  <div class="text-overlay">
      <h2>Welcome to CSE Motors!</h2>
      <%- messages() %>
      <h3>DMC Delorean</h3>
      <p>3 Cup holders</p>
      <p>Superman doors</p>
      <p>Fuzzy dice!</p>
      <h4 class="call-to-action">
          <span class="cta-box">Own Today</span>
      </h4>
  </div> <!-- This closing div was missing -->
</section>

<div class="sections-container">
  <!-- Upgrades Section -->
  <section class="upgrades-section">
    <h2>Delorean Upgrades</h2>
    <div class="upgrades">
      <!-- First Row -->
      <div class="upgrade-item">
        <img src="/images/upgrades/flux-cap.png" alt="Icon of a Flux Capacitor">
        <a href="#">Flux Capacitor</a>
      </div>
      <div class="upgrade-item">
        <img src="/images/upgrades/hub-cap.jpg" alt="Image of stylish Hub Caps">
        <a href="#">Hub Caps</a>
      </div>

      <!-- Second Row -->
      <div class="upgrade-item">
        <img src="/images/upgrades/flame.jpg" alt="Image of a Flame Decal on a car">
        <a href="#">Flame Decals</a>
      </div>
      <div class="upgrade-item">
        <img src="/images/upgrades/bumper_sticker.jpg" alt="Image of a Bumper Sticker">
        <a href="#">Bumper Stickers</a>
      </div>
    </div>
  </section>

  <!-- Reviews Section -->
  <section class="reviews">
    <h2>DMC Delorean Reviews</h2>
    <ul>
      <li>"So fast it's almost like traveling in time." (4/5)</li>
      <li>"Coolest ride on the road." (4/5)</li>
      <li>"I'm feeling McFly!" (5/5)</li>
      <li>"The most futuristic ride of our day." (4.5/5)</li>
      <li>"80's livin and I love it!" (5/5)</li>
    </ul>
  </section>
</div>